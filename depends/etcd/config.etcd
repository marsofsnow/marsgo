# Use goreman to run `go get github.com/mattn/goreman`
etcd1: etcd --data-dir=./d1 --name infra1 --listen-client-urls http://0.0.0.0:2379  --advertise-client-urls http://0.0.0.0:2379  --listen-peer-urls http://0.0.0.0:12380 --initial-advertise-peer-urls http://0.0.0.0:12380  --initial-cluster-token etcd-cluster-1  --initial-cluster 'infra1=http://0.0.0.0:12380,infra2=http://0.0.0.0:22380,infra3=http://0.0.0.0:32380'  --initial-cluster-state new   --enable-pprof    --logger=zap     --log-outputs=stderr
etcd2: etcd --data-dir=./d2 --name infra2 --listen-client-urls http://0.0.0.0:22379 --advertise-client-urls http://0.0.0.0:22379 --listen-peer-urls http://0.0.0.0:22380 --initial-advertise-peer-urls http://0.0.0.0:22380  --initial-cluster-token etcd-cluster-1  --initial-cluster 'infra1=http://0.0.0.0:12380,infra2=http://0.0.0.0:22380,infra3=http://0.0.0.0:32380'  --initial-cluster-state new   --enable-pprof    --logger=zap     --log-outputs=stderr
etcd3: etcd --data-dir=./d3 --name infra3 --listen-client-urls http://0.0.0.0:32379 --advertise-client-urls http://0.0.0.0:32379 --listen-peer-urls http://0.0.0.0:32380 --initial-advertise-peer-urls http://0.0.0.0:32380  --initial-cluster-token etcd-cluster-1  --initial-cluster 'infra1=http://0.0.0.0:12380,infra2=http://0.0.0.0:22380,infra3=http://0.0.0.0:32380'  --initial-cluster-state new   --enable-pprof    --logger=zap     --log-outputs=stderr

#proxy: etcd grpc-proxy start --endpoints=127.0.0.1:2379,127.0.0.1:22379,127.0.0.1:32379 --listen-addr=127.0.0.1:23790 --advertise-client-url=127.0.0.1:23790 --enable-pprof